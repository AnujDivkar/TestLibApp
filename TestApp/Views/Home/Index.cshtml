@model FiboModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Fibonacci Numbers</h1>
    <p>
        <form method="get">
        <input type="number" asp-for="Places" name="Places" id="Places" min="1" max="50">
        <br/>
        <input type="submit" value="Generate" class="btn btn-primary btn-sm" asp-controller="Home" asp-action="Index" />
        </form>
        <br />
        @ViewBag.Message
    </p>
</div>
